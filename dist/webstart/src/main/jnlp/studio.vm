<?xml version="1.0" encoding="$encoding"?>
<jnlp
    spec="$jnlpspec"
    codebase="http://www.renjin.org/webstart"
    href="http://www.renjin.org/webstart/studio.jnlp">
  <information>
    <title>Renjin Studio</title>
    <vendor>BeDataDriven</vendor>
    <homepage href="http://www.renjin.org"/>
    <description>Renjin Studio</description>
    <offline-allowed/>
    <shortcut online="false">
      <desktop/>
      <menu submenu="Renjin"/>
    </shortcut>
  </information>
  <security>
    <all-permissions/>
  </security>
  <resources>
    <j2se version="$j2seVersion"/>
    $dependencies
  </resources>
  <application-desc main-class="$mainClass"/>
</jnlp>
